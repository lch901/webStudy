drop table member2;

create table member2(
	id varchar2(10) primary key,
	name varchar2(10),
	pwd varchar2(10),
	age number(5),
	juso varchar2(50)
);

insert into member2(id,name,pwd,age,juso)values('1','lee','1234',26,'대구');
insert into member2(id,name,pwd,age,juso)values('2','jang','1234',33,'대구');
delete from member2;
select * from member2;